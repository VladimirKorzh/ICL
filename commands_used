# installing curl
  sudo apt-get install curl

# installing python setuptools
  wget https://bitbucket.org/pypa/setuptools/raw/bootstrap/ez_setup.py
  python ez_setup.py --user
 
# installing pip for python 
  curl -O https://raw.github.com/pypa/pip/master/contrib/get-pip.py
  sudo python get-pip.py
 
# installing Django 
  sudo pip install Django
 
# check that Django works
  python -c "import django; print(django.get_version())"
 
# installing mumble and murmur
  sudo apt-get install mumble mumble-server
 
# installing Ice
  sudo apt-get install zeroc-ice
   
# install github
  sudo apt-get install git
  
# install django-social-auth module
  sudo pip install django-social-auth
  sudo apt-get install python-openid python-oauth2
  sudo pip install pytz  
  
# install sqlite3 for database administration
  sudo apt-get install sqlite3
  
# install nodejs for bot and node sqlite3
  sudo apt-get update
  sudo apt-get install python-software-properties python g++ make
  sudo add-apt-repository ppa:chris-lea/node.js
  sudo apt-get update
  sudo apt-get install nodejs

  
# in bot directory you will need node-steam-trade project aside of
# others that are installed with the regular command
  npm install git://github.com/seishun/node-steam-trade.git
  npm install sqlite3
  npm install
  
  
